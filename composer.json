{
    "require": {
        "symfony/console": "^6.3",
        "symfony/flex": "^2.3",
        "symfony/validator": "^6.3",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.16",
        "symfony/runtime": "^6.3",
        "symfony/yaml": "^6.3",
        "symfony/dotenv": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        },
        "classmap": ["src"]
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.50"
    }
}
